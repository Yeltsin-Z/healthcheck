    "metrics.New_Pipeline_BPO_S0 = metrics.New_Pipeline_BPO_Source * metrics.New_Pipeline_BPO_S0_rate",
    "metrics.New_Pipeline_SDR_New_Opportunities = metrics.New_Pipeline_MQL_S0 + metrics.New_Pipeline_ABM_S0 + metrics.New_Pipeline_OB_S0",
    "metrics.New_Pipeline_AE_New_Opportunities = metrics.New_Pipeline_AE_S0",
    "metrics.New_Pipeline_References_New_Opportunities = metrics.New_Pipeline_Reference_S0",
    "metrics.New_Pipeline_Channel_New_Opportunities = metrics.New_Pipeline_Channel_S0 + metrics.New_Pipeline_OEM_S0 + metrics.New_Pipeline_BPO_S0",

    "metrics.New_Pipeline_SDR = metrics.New_Pipeline_SDR_New_Opportunities * metrics.Deal_Size",
    "metrics.New_Pipeline_AE = metrics.New_Pipeline_AE_New_Opportunities * metrics.Deal_Size",
    "metrics.New_Pipeline_References = metrics.New_Pipeline_References_New_Opportunities * metrics.Deal_Size",
    "metrics.New_Pipeline_Channel = metrics.New_Pipeline_Channel_New_Opportunities * metrics.Deal_Size",

    "metrics.New_Pipeline = metrics.New_Pipeline_SDR + metrics.New_Pipeline_AE + metrics.New_Pipeline_References + metrics.New_Pipeline_Channel",

    "metrics.Pipeline_Early[t=t] = metrics.Pipeline_Early[t=t-1] * ( 1 - metrics.Pipeline_Progression_Ratio_Early_Loss[t=t] - metrics.Pipeline_Progression_Ratio_Early_Mid[t=t] ) + metrics.New_Pipeline[t=t]",
    "metrics.Pipeline_Mid[t=t] = metrics.Pipeline_Mid[t=t-1] * ( 1 - metrics.Pipeline_Progression_Ratio_Mid_Loss[t=t] - metrics.Pipeline_Progression_Ratio_Mid_Late[t=t] ) + metrics.Pipeline_Early[t=t-1] * metrics.Pipeline_Progression_Ratio_Early_Mid[t=t]",
    "metrics.Pipeline_Late[t=t] = metrics.Pipeline_Late[t=t-1] * ( 1 - metrics.Pipeline_Progression_Ratio_Late_Loss[t=t] - metrics.Pipeline_Progression_Ratio_Late_Won[t=t] ) + metrics.Pipeline_Mid[t=t-1] * metrics.Pipeline_Progression_Ratio_Mid_Late[t=t]",

    "metrics.Closed_Won[t=t] = metrics.Pipeline_Late[t=t-1] * metrics.Pipeline_Progression_Ratio_Late_Won[t=t]",
    "metrics.Closed_Lost_Early[t=t] = metrics.Pipeline_Early[t=t-1] * metrics.Pipeline_Progression_Ratio_Early_Loss[t=t]",
    "metrics.Closed_Lost_Mid[t=t] = metrics.Pipeline_Mid[t=t-1] * metrics.Pipeline_Progression_Ratio_Mid_Loss[t=t]",
    "metrics.Closed_Lost_Late[t=t] = metrics.Pipeline_Late[t=t-1] * metrics.Pipeline_Progression_Ratio_Late_Loss[t=t]",

    "metrics.Pipeline = metrics.Pipeline_Early + metrics.Pipeline_Mid + metrics.Pipeline_Late",
    "metrics.Closed_Lost = metrics.Closed_Lost_Early + metrics.Closed_Lost_Mid + metrics.Closed_Lost_Late"
  ],
